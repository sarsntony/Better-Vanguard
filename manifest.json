{
  "name": "Better Vanguard",
  "version": "1.0",
  "description": "Improvements on Vanguard UI",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["https://personal.vanguard.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["contextMenus"]
}